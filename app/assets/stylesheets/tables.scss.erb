.table {
   .frame {
      border: dotted gray 1px;
      margin: 0px 4px 4px 4px;
      text-align: justify;
      padding: 2px;

      table {
         font-size: 8pt;
         width: 100%;
      }

      ul {
         padding-left: 10px;

         li {
            width: 100%;
         }
      }

      .left {
         &.float {
            float: left;
         }

         text-align: left;
      }

      .right {
         &.float {
            float: right;
         }

         text-align: right;
      }
   }
}
